<FormControl id="multiple-limit-tags">
  <FormLabel>limitTags</FormLabel>
  <Autocomplete
    multiple
    placeholder="Favorites"
    limitTags={2}
    options={top100Films}
    getOptionLabel={(option) => option.title}
    defaultValue={[top100Films[13], top100Films[12], top100Films[11]]}
    sx={{ width: '500px' }}
  />
</FormControl>