        />
    }
    >
    <div style={itemStyle}>reminder 3</div>
    <div style={itemStyle}>reminder 4</div>
    </Timeline.Item>
    <Timeline.Item
    label='August'
    dot={
        <Icon
        icon={faCalendarDay}
        style={{color: 'white', height: '50%'}}
        />
    }
    >
    <div style={itemStyle}>reminder 5</div>
    <div style={itemStyle}>reminder 6</div>
    <div style={itemStyle}>reminder 7</div>
    </Timeline.Item>
</Timeline>