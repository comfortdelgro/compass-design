    label='June'
    dot={
        <Icon
        icon={faCalendarDay}
        style={{color: 'white', height: '50%'}}
        />
    }
    >
    <div style={itemStyle}>reminder 2</div>
    <div style={itemStyle}>reminder 2</div>
