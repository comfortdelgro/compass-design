<AutoComplete
  searchedValue={inputValue}
  debounce={300}
  onSearch={(value) => onSearchHandler(value)}
  options={searchResults}
  onSelect={(value) => onSelectHandler(value)}
>
  <SearchField
    placeholder='Search'
    onChange={(newValue) => setInputValue(newValue)}
    value={inputValue}
    css={{width: '10rem'}}
  />
</AutoComplete>