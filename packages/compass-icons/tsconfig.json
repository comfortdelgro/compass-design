{
  "$schema": "https://json.schemastore.org/tsconfig",
  "compilerOptions": {
    "checkJs": true,
    "esModuleInterop": true,
    "importsNotUsedAsValues": "preserve",
    "jsx": "react-jsx",
    "lib": ["dom", "ES2021"],
    "module": "ESNext",
    "moduleResolution": "node",
    "skipLibCheck": true,
    "strict": false,
    "target": "ES2021"
  },
  "include": ["react/**/*.tsx"]
}
